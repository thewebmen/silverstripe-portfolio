{
    "name": "wedevelopnl/silverstripe-portfolio",
    "description": "Portfolio module",
    "keywords": [
        "silverstripe",
        "portfolio"
    ],
    "authors": [{
        "name": "WeDevelop",
        "email": "development@wedevelop.nl"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "silverstripe/framework": "^5.0",
        "silverstripe/vendor-plugin": "^2.0",
        "silverstripe/lumberjack": "^3.0",
        "silverstripe/tagfield": "^3.0",
        "unclecheese/display-logic": "^3.0",
        "wedevelopnl/silverstripe-gridfieldsitetreebuttons": "^1.0",
        "symbiote/silverstripe-gridfieldextensions": "^4.0"
    },
    "require-dev": {
      "friendsofphp/php-cs-fixer": "^3.20"
    },
    "extra": {
        "installer-name": "silverstripe-portfolio",
        "branch-alias": {
            "dev-main": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "WeDevelop\\Portfolio\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": false,
            "silverstripe/vendor-plugin": true,
            "silverstripe/recipe-plugin": false
        }
    }
}
